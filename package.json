{
  "name": "job-search-manager",
  "version": "1.0.0",
  "description": "A comprehensive job search management application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "server:dev": "nodemon",
    "server:build": "tsc",
    "client": "cd client && npm run dev",
    "build": "npm run server:build && cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node dist/index.js"
  },
  "keywords": ["job-search", "tracker", "management"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
